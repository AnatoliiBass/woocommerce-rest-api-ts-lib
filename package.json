{
  "name": "woocommerce-rest-ts-api",
  "version": "0.1.0",
  "description": "WooCommerce REST API - Type Script Library",
  "author": "Yuri Lima",
  "license": "MIT",
  "keywords": [
    "wordpress",
    "woocommerce",
    "rest",
    "promise",
    "node"
  ],
  "homepage": "https://github.com/Yuri-Lima/woocommerce-rest-api-ts-lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yuri-Lima/woocommerce-rest-api-ts-lib.git"
  },
  "bugs": {
    "url": "https://github.com/Yuri-Lima/woocommerce-rest-api-ts-lib/issues"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "index.ts",
    "index.d.ts"
  ],
  "dependencies": {
    "axios": "^1.1.3",
    "eslint-plugin-n": "^15.5.1",
    "oauth-1.0a": "^2.2.6",
    "typescript": "^4.8.4",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@types/create-hmac": "^1.1.0",
    "@types/jest": "^29.2.3",
    "@types/url-parse": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "13.0.3",
    "nock": "13.2.9",
    "npm-check-updates": "^16.4.1",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3"
  },
  "scripts": {
    "build": "del ./dist && npx tsc",
    "dep:update": "ncu && ncu --doctor -u && ncu -u",
    "test": "del ./dist && npx tsc && jest",
    "format": "prettier --write \"**/*.ts\" \"test.js\"",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A",
    "postversion": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
