{
  "name": "woocommerce-rest-ts-api",
  "version": "0.0.0-development",
  "description": "WooCommerce REST API - Type Script Library",
  "author": "Yuri Lima",
  "license": "MIT",
  "keywords": [
    "wordpress",
    "woocommerce",
    "rest",
    "promise",
    "node",
    "typescript",
    "api"
  ],
  "homepage": "https://github.com/Yuri-Lima/woocommerce-rest-api-ts-lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yuri-Lima/woocommerce-rest-api-ts-lib"
  },
  "bugs": {
    "url": "https://github.com/Yuri-Lima/woocommerce-rest-api-ts-lib/issues"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "oauth-1.0a": "^2.2.6",
    "typescript": "^4.9.3",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.6",
    "@types/jest": "^29.2.3",
    "@types/url-parse": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "lint-staged": "13.0.3",
    "nock": "13.2.9",
    "npm-check-updates": "^16.4.1",
    "prettier": "2.7.1",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "build": "npm run clean && npm run prepack",
    "dep:update": "ncu && ncu --doctor -u && ncu -u",
    "test": "jest",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run dep:update && npm run lint-and-fix",
    "clean": "rm -rf ./lib",
    "build:esm:cjs": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "npx tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "npx tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build:esm:cjs",
    "semantic-release": "semantic-release",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "types": "lib/cjs/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
